@{
    ViewBag.Title = "Error";
    Layout = "~/Views/Shared/_administrationLayout.cshtml";
}
<div id="layoutError">
    <div id="layoutError_content">
        <main>
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-6">
                        <div class="text-center mt-4">
                            <img width="300" src="~/pic/notFound.svg" />
                            <p class="lead">This requested URL was not found on this server.</p>
                            <a href='@Url.Action("GetListUser")'><i class="fas fa-arrow-left mr-1"></i>Return to List Client</a>
                        </div>
                    </div>
                </div>
            </div>
        </main>
    </div>
</div>