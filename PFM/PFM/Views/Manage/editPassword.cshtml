@model PFM.Models.ApplicationUser
@{
    ViewBag.title = "Password Manage";
}
@if (ViewData["PasswordExist"] == null)
{
    @Url.Action("passwordSettings");

}
else
{
    if (ViewData["PasswordExist"].ToString() == "yes")
    {
        @Html.Partial("ChangePassword");
    }
    else if (ViewData["PasswordExist"].ToString() == "no")
    {
        @Html.Partial("SetPassword");
    }

}