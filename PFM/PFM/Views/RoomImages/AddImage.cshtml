@model IEnumerable<PFM.Models.ModelsReservation.RoomImage>
@{
    ViewBag.Title = "AddImage";
}

<h2>AddImage</h2>

<div class="row">

    <div class="col-md-10">
        @using (Html.BeginForm("AddImage", "RoomImages", FormMethod.Post, new { @enctype = "multipart/form-data" }))
        {
            <input type="file" name="file" />
            <br />
            <input type="submit" value="Save" />
        }
    </div>

</div>

